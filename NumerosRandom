<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <title>gerar numero</title>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <!-- <link rel='stylesheet' type='text/css' media='screen' href='main.css'> -->
    <script>
    
    //Função gerar numero aleatorio do numero min ate o numero max-1//
    function getRandom(min, max) {
        vetor = new Array();
        aux = "";
        
        for(i = 0; i < 6; i++){
            
            min = Math.ceil(1);
            max = Math.floor(11);
            
            aux = Math.floor(Math.random() * (max - min)) + min;
            
            vetor.push(aux);
        
        document.getElementById("textarea").value = vetor;
        

    }
}

    </script>
</head>
<body>

    <form>
        <input type="button" value="GERAR" onclick="getRandom()"><br>
        <br>
        <textarea id="textarea">

        </textarea>
    </form>
    
</body>
</html>
